@model List<Colet>

<h2>Istoric de Colete</h2>

<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Destinatar</th>
            <th>Stare</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var colet in Model)
        {
            <tr>
                <td>@colet.ColetId</td>
                <td>@colet.Destinatar</td>
                <td>@colet.Stare</td>
            </tr>
        }
    </tbody>
</table>